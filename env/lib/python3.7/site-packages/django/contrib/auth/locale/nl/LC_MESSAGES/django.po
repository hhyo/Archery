msgstr "salt"

msgid "hash"
msgstr "hash"

msgid "variety"
msgstr "variatie"

msgid "version"
msgstr "versie"

msgid "memory cost"
msgstr "geheugenkosten"

msgid "time cost"
msgstr "tijdskosten"

msgid "parallelism"
msgstr "evenwijdigheid"

msgid "work factor"
msgstr "arbeidsfactor"

msgid "checksum"
msgstr "checksum"

